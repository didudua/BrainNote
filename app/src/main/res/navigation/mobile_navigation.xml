<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_create">

    <fragment
        android:id="@+id/nav_create"
        android:name="Lop48K14_1.group2.brainnote.ui.Home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_notes2"
        android:name="Lop48K14_1.group2.brainnote.ui.Home.NoteDetailFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_notes" />

    <fragment
        android:id="@+id/nav_notebooks"
        android:name="Lop48K14_1.group2.brainnote.ui.Home.NotebooksFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_notebooks" >
        <action
            android:id="@+id/action_nav_notebooks_to_new_NoteBook"
            app:destination="@id/new_NoteBook" />
    </fragment>
    <fragment
        android:id="@+id/nav_tasks"
        android:name="Lop48K14_1.group2.brainnote.ui.Home.TaskFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_task" />
    <fragment
        android:id="@+id/new_NoteBook"
        android:name="Lop48K14_1.group2.brainnote.ui.Home.NewNotebookFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_new_notebook" />
</navigation>